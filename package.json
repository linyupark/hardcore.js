{
  "name": "hardcore",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "dev": "onchange ./src/* -- npm run rollup & onchange ./build/bundle.es6.js -- npm run babel",
    "prerollup": "reset && eslint ./src/*",
    "rollup": "rollup ./src/index.es6.js -o ./build/bundle.es6.js -f iife -n window",
    "babel": "babel -o ./build/bundle.js ./build/bundle.es6.js",
    "uglifyjs": "uglifyjs ./build/bundle.js -m -o ./dist/wylib.min.js --source-map ./dist/wylib.min.map",
    "dist": "npm run rollup && npm run babel && npm run uglifyjs"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015-script": "^1.1.0",
    "onchange": "^3.0.2"
  }
}
